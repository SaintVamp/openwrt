config interface loopback
	option ifname	lo
	option proto	static
	option ipaddr	127.0.0.1
	option netmask	255.0.0.0

config interface lan
	option ifname	eth0
	option type 	none
	option proto	static
	option ipaddr	4.0.4.31
	option netmask	255.255.255.0
	option gateway  4.0.4.1
	option dns      114.114.114.114 8.8.8.8

config interface debug
	option ifname 	usb0
	option type 	none
	option proto 	static
	option ipaddr	172.18.0.18
	option netmask	255.255.255.0

